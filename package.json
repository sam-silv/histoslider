{
  "name": "histoslider",
  "version": "0.0.4",
  "description": "A slider component integrating a histogram, for React.",
  "main": "lib/index.js",
  "scripts": {
    "test": "standard src",
    "start": "webpack-dev-server --content-base docs/ --inline --hot",
