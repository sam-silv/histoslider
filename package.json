  "repository": "https://github.com/samhogg/histoslider",
  "author": "Sam Hogg",
  "license": "MIT",
  "devDependencies": {
    "babel-cli": "^6.14.0",
    "babel-core": "^6.14.0",
    "babel-loader": "^6.2.7",
    "babel-plugin-add-module-exports": "^0.2.1",
    "babel-preset-es2015": "^6.14.0",
    "babel-preset-react": "^6.11.1",
    "babel-preset-stage-0": "^6.5.0",
    "react": "^15.3.1",
    "react-dom": "^15.3.1",
    "standard": "^8.0.0",
    "webpack": "^2.2.1",
    "webpack-dev-server": "^2.2.1"
  },
  "dependencies": {
    "d3-array": "^1.0.2",
    "d3-format": "^1.0.2",
    "d3-scale": "^1.0.0"
